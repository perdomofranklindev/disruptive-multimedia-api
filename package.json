{
	"author": "Franklin Perdomo",
	"name": "disruptive-multimedia-api",
	"version": "1.0.0",
	"main": "src/app.ts",
	"scripts": {
		"dev": "nodemon src/app.ts",
		"start": "node dist/app.js",
		"build": "tsc --build --clean && tsc",
		"eslint": "eslint --ext .ts --fix ./src/ --ignore-path .gitignore",
		"format": "prettier --write \"./**/*.{js,json}\"",
		"clean:install": "rm -rf node_modules package-lock.json && npm cache clean --force && npm cache verify && npm i"
	},
	"dependencies": {
		"@prisma/client": "^5.13.0",
		"bcrypt": "^5.1.1",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"prisma": "^5.13.0",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.2",
		"@types/express": "^4.17.21",
		"@types/node": "^20.12.11",
		"@typescript-eslint/eslint-plugin": "^7.8.0",
		"@typescript-eslint/parser": "^7.8.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"nodemon": "^3.1.0",
		"prettier": "^3.2.5",
		"prisma-generator-typescript-interfaces": "^1.5.0",
		"ts-node": "^10.9.2",
		"typescript": "^5.4.5",
		"typescript-eslint": "^7.8.0"
	}
}
